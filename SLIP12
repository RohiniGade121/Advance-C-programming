/* accept string and compare both strings.*/
#include<stdio.h>
#include<string.h>
void main()
{
   int k;
   char s1[20],s2[20];
    printf("enter first string");
    gets(s1);
    printf("enter second string");
    gets(s2);
    k=strcmp(s1,s2);
     if(k==0)
      printf("both strings are same");
     else if(k>=0)
     printf("first string is greater"); 
     else 
      printf("second string is greater"); 
}
