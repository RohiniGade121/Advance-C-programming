 /*write a program to perform the following operation on two strings using standard library functions a.copy b.compare */
#include<stdio.h>
#include<string.h>
void main()
{
    char s1[20],s2[20];
    int k;
    printf("enter first string");
    gets(s1);
    printf("enter second string");
    gets(s2);
    strcpy(s1,s2);
     printf("first string=%s",s1);
      printf("second string=%s",s2);
    k=strcmp(s1,s2);
     if(k=0)
      printf("Both strings are equal");
     else if(k>0)
      printf("First string is greater");
     else 
      printf("second string is greater"); 
       

}
