#include<stdio.h>
 void main()
 {
    char s1[20],s2[20];
     int i,c1=0,c2=0;
      printf("enter first string");
      gets(s1);
      printf("enter second string");
      gets(s2);
       for(i=0;s1[i]!='\0';i++)
       {
        c1++;
       }
        for(i=0;s2[i]!='\0';i++)
         {
          c2++;
         }
         if(c1>c2)
          printf("%s is greater string",s1);
         else
          printf("%s is greater string",s2); 
 }
